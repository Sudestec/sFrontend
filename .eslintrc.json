{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["solid"],
  "extends": ["eslint:recommended", "plugin:solid/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-multi-spaces": "error",
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
